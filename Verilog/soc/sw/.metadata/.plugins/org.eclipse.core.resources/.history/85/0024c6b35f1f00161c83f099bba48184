#ifndef REG_DEFINE_H_
#define REG_DEFINE_H_

#define TIMER_LOAD_REG  0x00008000
#define TIMER_CUR_VALUE 0x00008004
#define TIMER_CTL_REG   0x00008008

#define PARITYSELECT 0x0001a000
#define DIN 0x0001a008
#define Config 0x0001a00C


#endif /* REG_DEFINE_H_ */
